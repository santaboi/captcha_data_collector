				$('#show_sp_msg').find('.modal-title').html(show_title);
				$('#show_sp_msg').find('#show_sp_msg_text').html(nowmsg_show);
				$('#show_sp_msg').modal();
			}
			function show_login_timeout(nowmsg){	
				if(nowmsg==undefined){
					nowmsg='操作已逾時，請重新登入！';
				}
				$('#login_timeout_msg').find('.modal-title').html('訊息通知');
				$('#login_timeout_msg').find('#login_timeout_text').html(nowmsg);
				$('#login_timeout_msg').modal();
				throw new Error(nowmsg);
			}
			function show_logout(){	
				$('#login_timeout_msg').find('.modal-title').html('訊息通知');
				$('#login_timeout_msg').find('#login_timeout_text').html('您已於其他分頁或視窗登出系統！');
