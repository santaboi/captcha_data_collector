			function show_note_msg(nowmsg, type , show_title){	
				nowmsg_show=nowmsg.replace(/\n/g,'<br>');
				if(show_title==undefined){
					show_title='訊息通知';
				}
				var show_note_style1="tc-note-danger tc-note-success tc-note-warning";
				var show_note_style2="fa-exclamation-circle fa-check-circle fa-info-circle";
				nowmsg_type = (type!=undefined&&(type==0||type==1||type=='0'||type=='1'))?((parseInt(type)==1)?'1':'0'):'2';
				if(nowmsg_type=='0'){
					$("#show_note_msg").find(".modal-content").removeClass(show_note_style1).addClass("tc-note-danger");
					$("#show_note_msg").find(".note-icon").find(".fa").removeClass(show_note_style2).addClass("fa-exclamation-circle");
				}else if(nowmsg_type=='1'){
					$("#show_note_msg").find(".modal-content").removeClass(show_note_style1).addClass("tc-note-success");
					$("#show_note_msg").find(".note-icon").find(".fa").removeClass(show_note_style2).addClass("fa-check-circle");
				}else{
					$("#show_note_msg").find(".modal-content").removeClass(show_note_style1).addClass("tc-note-warning");
					$("#show_note_msg").find(".note-icon").find(".fa").removeClass(show_note_style2).addClass("fa-info-circle");
				}
				if(nowmsg.indexOf("<br><br>") >= 0 ){
					$("#show_note_msg").find(".modal-dialog").removeClass("reveal-modal").addClass("reveal-modal2");
				}else{
					$("#show_note_msg").find(".modal-dialog").removeClass("reveal-modal2").addClass("reveal-modal");
				}
				$('#show_note_msg').find('.modal-title').html(show_title);
				$('#show_note_msg').find('#show_note_msg_text').html(nowmsg_show);
				$('#show_note_msg').modal();
			}
			function show_sp_msg(nowmsg, type , show_title){	
				nowmsg_show=nowmsg.replace(/\n/g,'<br>');
				if(show_title==undefined){
					show_title='訊息通知';
				}
				var show_note_style1="tc-note-danger tc-note-success tc-note-warning";
				var show_note_style2="fa-exclamation-circle fa-check-circle fa-info-circle";
				nowmsg_type = (type!=undefined&&(type==0||type==1||type=='0'||type=='1'))?((parseInt(type)==1)?'1':'0'):'2';
				if(nowmsg_type=='0'){
					$("#show_sp_msg").find(".modal-content").removeClass(show_note_style1).addClass("tc-note-danger");
					$("#show_sp_msg").find(".note-icon").find(".fa").removeClass(show_note_style2).addClass("fa-exclamation-circle");
				}else if(nowmsg_type=='1'){
					$("#show_sp_msg").find(".modal-content").removeClass(show_note_style1).addClass("tc-note-success");
					$("#show_sp_msg").find(".note-icon").find(".fa").removeClass(show_note_style2).addClass("fa-check-circle");
				}else{
					$("#show_sp_msg").find(".modal-content").removeClass(show_note_style1).addClass("tc-note-warning");
					$("#show_sp_msg").find(".note-icon").find(".fa").removeClass(show_note_style2).addClass("fa-info-circle");
				}
				if(nowmsg.indexOf("<br><br>") >= 0 ){
					$("#show_sp_msg").find(".modal-dialog").removeClass("reveal-modal").addClass("reveal-modal2");
				}else{
					$("#show_sp_msg").find(".modal-dialog").removeClass("reveal-modal2").addClass("reveal-modal");
