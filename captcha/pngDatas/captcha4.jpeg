	});
	$("#passwd").keyup(function(e){
		if((e.keyCode ? e.keyCode : e.which) == 13) $("#LoginForm").submit();
	});

	$('#LoginForm').submit(function () {
		var user_id = $.trim($('#user_id').val());
		var passwd = $('#passwd').val();
		var code = $.trim($('#code').val());
		if (user_id  === '' || passwd  === '' ) {
			$("#submit_by_acpw_msg").show();
			show_note_msg("<span style='color:red;'>請輸入帳號與密碼！</span>","0");
			return false;
		}else if (code  === ''||!$.isNumeric(code)) {
				$("#submit_by_acpw_msg").show();
			show_note_msg("<span style='color:red;'>驗證碼不正確！</span>","0");
			return false;
			
		}		
	});
 	$("#user_id").filter_input({regex:'[A-Za-z0-9]'});
	$("#user_id").keyup(function(e){
		var str=$(this).val();
		str=str.replace(/([Ａ-Ｚａ-ｚ０-９＿．，＠－＋＊～：；（）　])/g,
			function ($0) {
			  	return $0.charCodeAt(0)!='12288'?String.fromCharCode($0.charCodeAt(0) - 65248):String.fromCharCode(32);
			}
		);
		str=str.toUpperCase();
		$(this).val(str);
		if((e.keyCode ? e.keyCode : e.which) == 13) $("#LoginForm").submit();
	});
	$("#code").filter_input({regex:'[A-Za-z0-9]'});
	$("#code").keyup(function(e){
		var str=$(this).val();
		str=str.replace(/([Ａ-Ｚａ-ｚ０-９＿．，＠－＋＊～：；（）　])/g,
			function ($0) {
			  	return $0.charCodeAt(0)!='12288'?String.fromCharCode($0.charCodeAt(0) - 65248):String.fromCharCode(32);
			}
		);
		$(this).val(str);
		if((e.keyCode ? e.keyCode : e.which) == 13) $("#LoginForm").submit();
	});
	$.removeCookie("SSO");
	$("#clock").clock({
		"timestamp":1630661577,
		"langSet":"zh-tw",
		"dateFormat":"Y-m-d ",
		"timeFormat":"H:i:s",
		"rate":50
	});

